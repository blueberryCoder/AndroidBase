<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.blueberry.sample">

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"></uses-permission>
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"></uses-permission>
    <uses-permission android:name="android.permission.INTERNET"></uses-permission>
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:name="com.blueberry.sample.app.App"
        android:theme="@style/AppTheme">
        <activity android:name="com.blueberry.sample.module.home.HomeActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.blueberry.sample.module.PageActivity"
            android:label="页面"
            >
        </activity>

        <activity android:name="com.blueberry.sample.module.view.srcoll_confict.ScrollConfictActivity"
            android:label="滑动冲突"
            >

        </activity>
    </application>

</manifest>